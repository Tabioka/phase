{
  "private": true,
  "name": "phasebot",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "phase": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./builders": {
      "import": "./dist/builders/index.js",
      "require": "./dist/builders/index.js"
    }
  },
  "scripts": {
    "build": "tsup ./src --minify --sourcemap --clean --no-splitting --dts --format=esm",
    "dev": "tsup ./src --minify --sourcemap --clean --no-splitting --dts --format=esm --watch",
    "postinstall": "pnpm run build"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cron": "^3.1.6",
    "discord-api-types": "^0.37.67",
    "discord.js": "^14.12.1",
    "dotenv-flow": "^4.1.0",
    "ora": "^8.0.1",
    "tsup": "^8.0.2"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.3.3",
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3"
  }
}
