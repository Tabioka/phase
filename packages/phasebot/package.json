{
  "name": "phasebot",
  "description": "A simple framework for building and running discord.js applications.",
  "version": "0.2.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src --minify --clean --dts --format=esm",
    "dev": "tsup ./src --minify --clean --dts --format=esm --watch",
    "postinstall": "node postinstall.js"
  },
  "bin": {
    "phase": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./builders": {
      "import": "./dist/builders/index.js",
      "require": "./dist/builders/index.js"
    }
  },
  "keywords": [
    "discord",
    "discordjs",
    "bot",
    "framework",
    "client",
    "node"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/notcharliee/phase.git"
  },
  "dependencies": {
    "@vercel/og": "^0.6.2",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cron": "^3.1.6",
    "discord.js": "^14.12.1",
    "dotenv-flow": "^4.1.0",
    "ora": "^8.0.1",
    "react": "18.2.0",
    "ts-mixer": "^6.0.4",
    "tsup": "^8.0.2"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.3.3",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.67",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "discord.js": "^14.x.x"
  }
}
