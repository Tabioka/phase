{
  "name": "phasebot",
  "description": "A simple framework for building and running discord bots",
  "version": "0.15.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./builders": {
      "import": "./dist/structures/builders/index.js",
      "require": "./dist/structures/builders/index.js",
      "types": "./dist/structures/builders/index.d.ts"
    },
    "./managers": {
      "import": "./dist/structures/managers/index.js",
      "require": "./dist/structures/managers/index.js",
      "types": "./dist/structures/managers/index.d.ts"
    },
    "./stores": {
      "import": "./dist/structures/stores/index.js",
      "require": "./dist/structures/stores/index.js",
      "types": "./dist/structures/stores/index.d.ts"
    }
  },
  "keywords": [
    "discord",
    "discordjs",
    "bot",
    "framework",
    "client",
    "bun"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notcharliee/phase.git"
  },
  "dependencies": {
    "@vercel/og": "^0.6.3",
    "chalk": "^5.3.0",
    "croner": "^8.1.1",
    "ora": "^8.1.0",
    "ts-mixer": "^6.0.4"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@types/bun": "^1.1.9",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "discord.js": "^14.x.x"
  }
}
